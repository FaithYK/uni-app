<view class="proj-item"><label class="{{['index _span',index<1?'active':'']}}">{{index}}</label><view class="info"><image class="logo" mode="aspectFit" src="{{info.logo}}"></image><view class="r"><view class="name">{{info.project_name}}</view><view class="time">{{info.online_time}}</view></view></view><view class="focus-num">{{info.focus_num}}</view><view class="focus"><block wx:if="{{info.is_focus}}"><image class="icon" src="../../static/img/proj/focus.png" data-event-opts="{{[['tap',[['already',['$event']]]]]}}" bindtap="__e"></image></block><block wx:else><image class="icon" src="../../static/img/proj/focus-no.png" data-event-opts="{{[['tap',[['focusFn',['$0','$1'],['info.project_id','index']]]]]}}" bindtap="__e"></image></block></view></view>